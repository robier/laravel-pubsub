{
  "name": "robier/laravel-pubsub",
  "description": "A Pub-Sub abstraction for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Superbalist.com a division of Takealot Online (Pty) Ltd",
      "email": "info@superbalist.com"
    }
  ],
  "conflict": {
    "superbalist/laravel-pubsub": "*",
    "entanet/laravel-pubsub": "*"
  },
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^6|^7|^8",
    "superbalist/php-pubsub": "^1.0|^2.0",
    "superbalist/php-pubsub-redis": "^1.0|^2.0",
    "superbalist/php-pubsub-google-cloud": "5.3.0-beta",
    "illuminate/console": "^6|^7|^8",
    "robier/php-pubsub-http": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Superbalist\\LaravelPubSub\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5"
  }
}
